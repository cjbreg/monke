{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "paths": {
    "@/*" : [
        "src/*"
    ],
    "@ui/*" : [
        "src/ui/*"
    ],
    "@components/*" : [
        "src/components/*"
    ],
  }
}
