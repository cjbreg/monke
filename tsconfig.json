{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "paths": {
    "@/*" : [
        "src/*"
    ],
    "@ui/*" : [
        "src/ui/*"
    ],
    "@components/*" : [
        "src/components/*"
    ],
    "@helpers/*" : [
        "src/helpers/*"
    ],
    "@providers/*" : [
        "src/providers/*"
    ],
    "@hooks/*" : [
        "src/hooks/*"
    ],
  }
}
